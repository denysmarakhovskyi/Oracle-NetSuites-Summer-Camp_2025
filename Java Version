public class BazInga {
    public static void main(String[] args) {
        for (int i = 0; i <= 100; i++) {
            String output = "";

            if (i % 2 == 0) {
                output += "Baz";
            }
            if (i % 5 == 0) {
                output += "inga!";
            }

            if (output.isEmpty()) {
                System.out.println(i);
            } else {
                System.out.println(output);
            }
        }
    }
}
